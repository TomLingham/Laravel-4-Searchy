{
    "name": "tom-lingham/searchy",
    "description": "Laravel Searchy makes user driven searching easy with fuzzy search, basic string matching, Levenshtein Distance and more.",
    "keywords": ["laravel","search","fuzzy"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lingham",
            "email": "tjlingham@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.*"
    },
    "require-dev" :{
        "graham-campbell/testbench": "^4.0",
        "mockery/mockery": "dev-master#c90a17247147543081e4d00f46911e422b49e583",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "TomLingham\\Searchy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomLingham\\Tests\\Searchy\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
